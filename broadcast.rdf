<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY DOE "http://cwi.nl/~troncy/DOE#" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY vann "http://purl.org/vocab/vann/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY event "http://purl.org/NET/c4dm/event.owl#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY agrovoc "http://www.fao.org/aims/aos/agrovoc/" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY tl "http://purl.org/NET/c4dm/timeline.owl#" >
    <!ENTITY part "http://purl.org/vocab/participation/schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY OntoPneumo "http://doe-generated-ontology.com/OntoPneumo#" >
    <!ENTITY generations "http://www.owl-ontologies.com/generations.owl#" >
    <!ENTITY carOntology "http://users.jyu.fi/~artkache/carOntology.owl#" >
    <!ENTITY pizza "http://www.co-ode.org/ontologies/pizza/pizza.owl#" >
    <!ENTITY DosageDuFacteurVII "http://doe-generated-ontology.com/OntoPneumo#DosageDuFacteurVII+" >
]>


<rdf:RDF xmlns="http://purl.org/broadcasts/programmes#"
     xml:base="http://purl.org/broadcasts/programmes"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:carOntology="http://users.jyu.fi/~artkache/carOntology.owl#"
     xmlns:tl="http://purl.org/NET/c4dm/timeline.owl#"
     xmlns:DosageDuFacteurVII="&OntoPneumo;DosageDuFacteurVII+"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:DOE="http://cwi.nl/~troncy/DOE#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:OntoPneumo="http://doe-generated-ontology.com/OntoPneumo#"
     xmlns:agrovoc="http://www.fao.org/aims/aos/agrovoc/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:generations="http://www.owl-ontologies.com/generations.owl#"
     xmlns:event="http://purl.org/NET/c4dm/event.owl#"
     xmlns:pizza="http://www.co-ode.org/ontologies/pizza/pizza.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:part="http://purl.org/vocab/participation/schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="http://purl.org/broadcasts/programmes#">
        <dc:description xml:lang="en">An Ontology for describing the various relations and classes envolved in Broadcastable Media</dc:description>
        <vann:preferredNamespacePrefix>bo</vann:preferredNamespacePrefix>
        <dc:creator xml:lang="en">Thomas Grainger</dc:creator>
        <vann:preferredNamespaceUri>http://purl.org/broadcasts/programmes#</vann:preferredNamespaceUri>
        <owl:imports rdf:resource="http://purl.org/NET/c4dm/event.owl#"/>
        <owl:imports rdf:resource="http://purl.org/NET/c4dm/timeline.owl#"/>
        <owl:imports rdf:resource="http://purl.org/dc/elements/1.1/"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <owl:imports rdf:resource="https://raw.github.com/graingert/participation-schema/865febf4a8820ecd487340b07134be52c3f8f614/participation.rdf"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="&vann;preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="&vann;preferredNamespaceUri"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#comment -->

    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/broadcasts/programmes#position -->

    <rdfs:Datatype rdf:about="http://purl.org/broadcasts/programmes#position">
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">1</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/c4dm/event.owl#factor -->

    <owl:ObjectProperty rdf:about="&event;factor"/>
    


    <!-- http://purl.org/NET/c4dm/event.owl#factor_of -->

    <owl:ObjectProperty rdf:about="&event;factor_of"/>
    


    <!-- http://purl.org/broadcasts/programmes#actsAt -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#actsAt">
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#hasActor"/>
        <rdfs:subPropertyOf rdf:resource="&part;role_at"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#directsAt -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#directsAt">
        <rdfs:subPropertyOf rdf:resource="&part;role_at"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasActor -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasActor">
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Actor"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Cast"/>
        <rdfs:subPropertyOf rdf:resource="&part;role"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasBroadcast -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasBroadcast">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">relates one Programme to some Broadcast event</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&event;factor_of"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Broadcast"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasBroadcaster -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasBroadcaster">
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Broadcaster"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Channel"/>
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#isBroadcasterOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasCast -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasCast">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">relates a Programme to the single Cast that made it</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Cast"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#isCastOf"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;maker"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasChannel -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasChannel">
        <rdfs:comment xml:lang="en">Is equivalent to isBroadcastOn to fit with the naming convention.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&event;factor"/>
        <owl:equivalentProperty rdf:resource="http://purl.org/broadcasts/programmes#isBroadcastOn"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasDirector -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasDirector">
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Cast"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Director"/>
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#directsAt"/>
        <rdfs:subPropertyOf rdf:resource="&part;role"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasEpisode -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasEpisode">
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#isEpisodeOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasFormat -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasFormat">
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Format"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#isFormatOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasGenre -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasGenre">
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Genre"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#isGenreOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasPresenter -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasPresenter">
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Cast"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Presenter"/>
        <rdfs:subPropertyOf rdf:resource="&part;role"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasProducer -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasProducer">
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Cast"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Producer"/>
        <rdfs:subPropertyOf rdf:resource="&part;role"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#hasProductionCompany -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#hasProductionCompany">
        <rdfs:comment xml:lang="en">relates the ProductionCompany that made the</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#ProductionCompany"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;maker"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#isBroadcastOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isBroadcastOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">relates some Broadcast to a Programme</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&event;factor"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Broadcast"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#hasBroadcast"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#isBroadcastOn -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isBroadcastOn">
        <rdfs:comment xml:lang="en">Relates a Broadcast to the Channel that it is Broadcast on</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&event;factor"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Broadcast"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Channel"/>
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#isChannelOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#isBroadcasterOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isBroadcasterOf">
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Broadcaster"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Channel"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#isCastOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isCastOf">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">relates a single Cast to a Programme that the cast made</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Cast"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;made"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#isChannelOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isChannelOf">
        <rdfs:subPropertyOf rdf:resource="&event;factor_of"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#isEpisodeOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isEpisodeOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Episode"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Series"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#isFormatOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isFormatOf">
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Format"/>
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#isGenreOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isGenreOf"/>
    


    <!-- http://purl.org/broadcasts/programmes#isProductionCompanyOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#isProductionCompanyOf">
        <rdfs:domain rdf:resource="http://purl.org/broadcasts/programmes#ProductionCompany"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#hasProductionCompany"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;made"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#presentsAt -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#presentsAt">
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#hasPresenter"/>
        <rdfs:subPropertyOf rdf:resource="&part;role_at"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/broadcasts/programmes#producesAt -->

    <owl:ObjectProperty rdf:about="http://purl.org/broadcasts/programmes#producesAt">
        <owl:inverseOf rdf:resource="http://purl.org/broadcasts/programmes#hasProducer"/>
        <rdfs:subPropertyOf rdf:resource="&part;role_at"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/participation/schema#holder -->

    <owl:ObjectProperty rdf:about="&part;holder"/>
    


    <!-- http://purl.org/vocab/participation/schema#holder_of -->

    <owl:ObjectProperty rdf:about="&part;holder_of"/>
    


    <!-- http://purl.org/vocab/participation/schema#role -->

    <owl:ObjectProperty rdf:about="&part;role"/>
    


    <!-- http://purl.org/vocab/participation/schema#role_at -->

    <owl:ObjectProperty rdf:about="&part;role_at"/>
    


    <!-- http://xmlns.com/foaf/0.1/made -->

    <owl:ObjectProperty rdf:about="&foaf;made"/>
    


    <!-- http://xmlns.com/foaf/0.1/maker -->

    <owl:ObjectProperty rdf:about="&foaf;maker"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/broadcasts/programmes#hasPosition -->

    <owl:DatatypeProperty rdf:about="http://purl.org/broadcasts/programmes#hasPosition">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="http://purl.org/broadcasts/programmes#position"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/broadcasts/programmes#Episode"/>
                    <rdf:Description rdf:about="http://purl.org/broadcasts/programmes#Series"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/c4dm/event.owl#Event -->

    <owl:Class rdf:about="&event;Event"/>
    


    <!-- http://purl.org/NET/c4dm/event.owl#Factor -->

    <owl:Class rdf:about="&event;Factor"/>
    


    <!-- http://purl.org/broadcasts/programmes#Actor -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#Actor">
        <rdfs:subClassOf rdf:resource="http://purl.org/broadcasts/programmes#RoleOnCast"/>
        <dc:description xml:lang="en">The Role of a person who acts in a dramatic or comic production and works in film, television, theatre, or radio in that capacity.</dc:description>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#ActorDirectorPerson -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#ActorDirectorPerson">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&foaf;Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&part;holder_of"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/broadcasts/programmes#Actor"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&part;holder_of"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/broadcasts/programmes#Director"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description xml:lang="en">A person who both acts and directs.</dc:description>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#Broadcast -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#Broadcast">
        <rdfs:subClassOf rdf:resource="&event;Event"/>
        <rdfs:comment xml:lang="en">Broadcast subclasses event so as to have a time and factors that are some Broacasters and a Programme.</rdfs:comment>
        <dc:description xml:lang="en">Event that describes the transmition of a programme for public or general use.</dc:description>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#Broadcaster -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#Broadcaster">
        <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
        <dc:description xml:lang="en">The Organization that handles the Broadcast of a Programme</dc:description>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#Cast -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#Cast">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&foaf;Group"/>
                    <rdf:Description rdf:about="&foaf;Organization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Cast is used because the Role ontology requires an Organization or Group and Programme is Disjoint with foaf:Agent.</rdfs:comment>
        <dc:description xml:lang="en">The Organisation or Group that made a Programme.</dc:description>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#Channel -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#Channel">
        <rdfs:subClassOf rdf:resource="&event;Factor"/>
        <rdfs:comment xml:lang="en">A Channel is the Factor that an Episode is Broadcast on</rdfs:comment>
        <dc:description xml:lang="en">A specified band for the transmission and reception of electromagnetic signals, as for television signals.</dc:description>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#CurrentAffairsProgramme -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#CurrentAffairsProgramme">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/broadcasts/programmes#Programme"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/broadcasts/programmes#hasGenre"/>
                        <owl:hasValue rdf:resource="http://purl.org/broadcasts/programmes#CurrentAffairsGenre"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description xml:lang="en">A current affairs programme.</dc:description>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#Director -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#Director">
        <rdfs:subClassOf rdf:resource="http://purl.org/broadcasts/programmes#RoleOnCast"/>
        <dc:description xml:lang="en">The Role of a person who directs the making of a Programme. Generally, the role consists of controling a film&apos;s artistic and dramatic aspects, and visualizing the script.</dc:description>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#Episode -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#Episode">
        <rdfs:subClassOf rdf:resource="http://purl.org/broadcasts/programmes#Programme"/>
        <owl:disjointWith rdf:resource="http://purl.org/broadcasts/programmes#Series"/>
        <dc:description xml:lang="en">A separate program that is part of a television or radio series.</dc:description>
        <rdfs:comment xml:lang="en">An episode is a Programme, that has a hasPosition in some Series.

This allows one off Programme instances that are not part of a Series to be defined.

A key is defined such that any Episode with the same position and Series is the same Episode.</rdfs:comment>
        <owl:hasKey rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/broadcasts/programmes#isEpisodeOf"/>
            <rdf:Description rdf:about="http://purl.org/broadcasts/programmes#hasPosition"/>
        </owl:hasKey>
    </owl:Class>
    


    <!-- http://purl.org/broadcasts/programmes#Format -->

    <owl:Class rdf:about="http://purl.org/broadcasts/programmes#Format">
