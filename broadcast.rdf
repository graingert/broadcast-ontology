        <rdfs:domain rdf:resource="&bo;Version"/>
        <rdfs:subPropertyOf rdf:resource="&event;factor"/>
    </owl:ObjectProperty>
    


    <!-- http://graingert.co.uk/broadcast-ontology/version -->

    <owl:ObjectProperty rdf:about="&bo;version">
