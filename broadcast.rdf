    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://graingert.co.uk/broadcast-ontology/Actor -->

    <owl:Class rdf:about="&bo;Actor">
        <rdfs:subClassOf rdf:resource="&foaf;Person"/>
    </owl:Class>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Broadcast -->

    <owl:Class rdf:about="&bo;Broadcast">
        <rdfs:subClassOf rdf:resource="&event;Event"/>
    </owl:Class>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Broadcaster -->

    <owl:Class rdf:about="&bo;Broadcaster">
        <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
    </owl:Class>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Channel -->

    <owl:Class rdf:about="&bo;Channel"/>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Director -->

    <owl:Class rdf:about="&bo;Director">
        <rdfs:subClassOf rdf:resource="&foaf;Person"/>
    </owl:Class>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Episode -->

    <owl:Class rdf:about="&bo;Episode"/>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Format -->

    <owl:Class rdf:about="&bo;Format"/>
    <owl:Class rdf:about="&bo;Genre"/>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Presenter -->

    <owl:Class rdf:about="&bo;Presenter">
        <rdfs:subClassOf rdf:resource="&foaf;Person"/>
    </owl:Class>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Producer -->

    <owl:Class rdf:about="&bo;Producer">
        <rdfs:subClassOf rdf:resource="&foaf;Person"/>
    </owl:Class>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Production_Company -->

    <owl:Class rdf:about="&bo;Production_Company">
        <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
    </owl:Class>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Programme -->

    <owl:Class rdf:about="&bo;Programme"/>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Series -->

    <owl:Class rdf:about="&bo;Series"/>
    


    <!-- http://graingert.co.uk/broadcast-ontology/Version -->

    <owl:Class rdf:about="&bo;Version"/>
    


