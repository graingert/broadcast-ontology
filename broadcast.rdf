     -->

    


    <!-- http://graingert.co.uk/broadcast-ontology/broadcast -->

    <owl:ObjectProperty rdf:about="&bo;hasBroadcast">
        <rdfs:range rdf:resource="&bo;Broadcast"/>
        <rdfs:domain rdf:resource="&bo;Version"/>
        <owl:inverseOf rdf:resource="&bo;isBroadcastOf"/>
        <rdfs:subPropertyOf rdf:resource="&event;factor_of"/>
    </owl:ObjectProperty>
    


    <!-- http://graingert.co.uk/broadcast-ontology/broadcast_of -->

    <owl:ObjectProperty rdf:about="&bo;isBroadcastOf">
        <rdfs:subPropertyOf rdf:resource="&event;factor"/>
    </owl:ObjectProperty>
    


    <!-- http://graingert.co.uk/broadcast-ontology/broadcast_on -->

    <owl:ObjectProperty rdf:about="&bo;isBroadcastOn">
        <rdfs:comment xml:lang="en">Relates a particular broadcast to a particular channel. A Broadcast can be simulcast on multiple Channels.</rdfs:comment>
        <rdfs:domain rdf:resource="&bo;Broadcast"/>
        <rdfs:range rdf:resource="&bo;Channel"/>
        <rdfs:subPropertyOf rdf:resource="&event;factor"/>
    </owl:ObjectProperty>


    <!-- http://graingert.co.uk/broadcast-ontology/version -->

    <owl:ObjectProperty rdf:about="&bo;hasVersion">
        <rdfs:domain rdf:resource="&bo;Programme"/>
        <rdfs:range rdf:resource="&bo;Version"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bo;isVersionOf">
        <owl:inverseOf rdf:resource="&bo;version"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bo;hasGenre">
        <rdfs:domain rdf:resource="&bo;Programme"/>
        <rdfs:range rdf:resource="&bo;Genre"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bo;isGenreOf">
        <owl:inverseOf rdf:resource="&bo;hasGenre"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bo;hasFormat">
        <rdfs:domain rdf:resource="&bo;Programme"/>
        <rdfs:range rdf:resource="&bo;Format"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&bo;isFormatOf">
        <owl:inverseOf rdf:resource="&bo;hasFormat"/>
    </owl:ObjectProperty>


    <!-- 
